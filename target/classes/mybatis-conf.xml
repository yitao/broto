<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<!-- configure your typealias -->
		<typeAlias type="com.broto.backstage.entity.Account" alias="Account" />
		<typeAlias type="com.broto.backstage.entity.Action" alias="Action" />
		<typeAlias type="com.broto.backstage.entity.Module" alias="Module" />
		<typeAlias type="com.broto.backstage.entity.Role" alias="Role" />
		<typeAlias type="com.broto.backstage.entity.RMA" alias="RMA" />
		<typeAlias type="com.broto.backstage.entity.AR" alias="AR" />
		<typeAlias type="com.broto.backstage.entity.R2R" alias="R2R" />
		<typeAlias type="com.broto.backstage.entity.R2MA" alias="R2MA" />
	</typeAliases>
	<mappers>
		<!-- configure your mapper -->
		<mapper resource="com/broto/backstage/entity/mapper/AccountMapper.xml"/>
		<mapper resource="com/broto/backstage/entity/mapper/ActionMapper.xml"/>
		<mapper resource="com/broto/backstage/entity/mapper/ModuleMapper.xml"/>
		<mapper resource="com/broto/backstage/entity/mapper/RoleMapper.xml"/>
		<mapper resource="com/broto/backstage/entity/mapper/RMAMapper.xml"/>
		<mapper resource="com/broto/backstage/entity/mapper/ARMapper.xml"/>
		<mapper resource="com/broto/backstage/entity/mapper/R2RMapper.xml"/>
		<mapper resource="com/broto/backstage/entity/mapper/R2MAMapper.xml"/>
	</mappers>

</configuration>